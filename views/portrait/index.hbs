<section class="content">
    {{#each thumbnails}}
        <div class="card">
            <a href="/portrait/{{ _id }}"><img src="{{ source }}" alt="{{ title }}" /></a>
            <a href="/portrait/{{ _id }}"><p><span>{{ title }}</span>{{#if year}}, {{ year }}{{/if}}</p></a>
        </div>
    {{/each}}
</section>